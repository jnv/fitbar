{
  "name": "fitbar",
  "version": "0.2.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "atomify --server [ --sync --path=/index.html ] --output=.build/fitbar",
    "build": "atomify --output=dist/fitbar",
    "build:ext": "atomify --output=ext/chrome/src/inject",
    "test": "tape src/test/*_test.js"
  },
  "dependencies": {
    "curry": "^1.2.0",
    "domready": "^1.0.7",
    "functional-pipeline": "git://github.com/jnv/functional-pipeline.git",
    "include-folder": "^0.7.0",
    "mithril": "^0.1.26",
    "rework-class-prefix": "git://github.com/jnv/rework-class-prefix.git",
    "rework-inherit": "^0.2.3",
    "rework-namespace": "^0.3.0",
    "rework-plugin-colors": "^1.0.3"
  },
  "devDependencies": {
    "atomify": "^6.0.0",
    "lodash": "^3.6.0",
    "resrcify": "^1.1.3",
    "folderify": "^0.6.0",
    "tape": "^3.5.0"
  },
  "atomify": {
    "assets": {
      "dest": "dist/assets",
      "prefix": "assets/"
    },
    "js": {
      "entry": "src/index.js",
      "alias": "/fitbar.js",
      "transforms": [
        "folderify"
      ]
    },
    "css": {
      "entry": "src/index.css",
      "alias": "/fitbar.css",
      "plugins": [
        [
          "rework-inherit"
        ],
        [
          "rework-class-prefix",
          "fitbar-",
          {
            "not": "/^is-/"
          }
        ],
        [
          "rework-plugin-colors"
        ]
      ]
    }
  }
}
