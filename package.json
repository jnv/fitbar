{
  "name": "fitbar",
  "version": "0.2.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "atomify --server [ --lr true --path index.html ] -d",
    "build": "atomify -o dist/fitbar"
  },
  "dependencies": {
    "mithril": "^0.1.22",
    "rework-inherit": "^0.2.3",
    "wrap-selectors": "^0.1.0"
  },
  "devDependencies": {
    "atomify": "^5.0.0",
    "karma-chai": "*",
    "karma-coverage": "*",
    "karma-mocha": "*",
    "karma-phantomjs-launcher": "*",
    "karma-sinon": "*",
    "mocha": "*",
    "sinon-chai": "*"
  },
  "atomify": {
    "js": {
      "entry": "src/index.js",
      "alias": "/bundle.js"
    },
    "css": {
      "entry": "src/index.css",
      "alias": "/bundle.css",
      "plugins": [
        [
          "wrap-selectors"
        ],
        [
          "rework-inherit"
        ]
      ]
    }
  }
}
